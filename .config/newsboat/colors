color listnormal        white   default
color listnormal_unread red     default bold
color listfocus         black   white   standout
color listfocus_unread  red     black   bold
color info              white   black   bold
color article           white   default bold
color listfocus yellow  default bold
color listfocus_unread  red     default bold
color listnormal_unread green   default bold

highlight all     "---.+---"                          white default bold
highlight all     ".+(0/0)) "                         black default bold
highlight article "^(Feed|Title|Author|Link|Date):.*" white default
highlight article "https?://[^ ]+"                    green
highlight article "\[ image [0-9]+\]"                 green default
highlight article "\[[0-9]+\]"                        green default
highlight article "^Links:"                           white default bold
